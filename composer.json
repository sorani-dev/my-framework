    "require": {
        "php": "<8",
        "guzzlehttp/psr7": "^1.8",
        "http-interop/response-sender": "^1.0",
        "mezzio/mezzio-fastroute": "^3.2",
        "twig/twig": "^3.4",
        "php-di/php-di": "^6.3",
        "robmorgan/phinx": "^0.12.6",
        "pagerfanta/pagerfanta": "^2",
        "intervention/image": "^2.5",
        "middlewares/whoops": "1.*"
    }

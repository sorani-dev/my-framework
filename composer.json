    "http-interop/response-sender": "^1.0",
    "mezzio/mezzio-fastroute": "^2.2",
    "twig/twig": "^1.42",
    "php-di/php-di": "^5.4",
    "robmorgan/phinx": "^0.11.7",
    "pagerfanta/pagerfanta": "^1.1",
    "intervention/image": "^2.5",
    "middlewares/whoops": "^0.5.1",
    "doctrine/cache": "^1.6",
    "altorouter/altorouter": "^2.0",
    "aura/router": "^3.1",
    "paragonie/random_compat": "^2.0"
